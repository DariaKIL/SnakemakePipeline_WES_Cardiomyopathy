rule filter_indel:
    input:
	    "raw_indels.vcf"
	    "ref"
    output:
	    "filtered_indels.vcf"
    shell: "~/Documents/Project2/gatk-4.1.0.0/gatk -T VariantFiltration -R ref -V raw_indels.vcf --filterExpression QD < 2.0 || FS > 200.0 || ReadPosRankSum < -20.0 --filterName my_indel_filter -o filtered_indels.vcf"